:CurrentDefinition a owl:Class ;
    rdfs:subClassOf :Definition ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :Definition [ a owl:Class ;
                        owl:complementOf [ a owl:Restriction ;
                                owl:onProperty :hasEndValidity ;
                                owl:someValuesFrom xsd:date ] ] ) ] .

:DefiningConstant a owl:Class .

:SIPrefixDivider a owl:Class ;
    rdfs:subClassOf :SIPrefix ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :SIPrefix [ a owl:Restriction ;
                        owl:onProperty :hasScalingFactor ;
                        owl:someValuesFrom [ a rdfs:Datatype ;
                                owl:onDatatype xsd:double ;
                                owl:withRestrictions ( [ xsd:maxExclusive 1e+00 ] ) ] ] ) ] .

:SIPrefixMultipliers a owl:Class ;
    rdfs:subClassOf :SIPrefix ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :SIPrefix [ a owl:Restriction ;
                        owl:onProperty :hasScalingFactor ;
                        owl:someValuesFrom [ a rdfs:Datatype ;
                                owl:onDatatype xsd:double ;
                                owl:withRestrictions ( [ xsd:minExclusive 1e+00 ] ) ] ] ) ] .

:SIPrefixedUnit a owl:Class ;

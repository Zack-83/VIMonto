@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vim: <http://test.vimrdf.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vim:BaseQuantity a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "quantity that in a given system of quantities is not defined in terms of the others" ;
    vim:it 3 ;
    vim:term "base quantity" ;
    rdfs:subClassOf vim:GeneralQuantity .

vim:BaseUnit a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "measurement unit that is adopted by convention for a base quantity" ;
    vim:it 13 ;
    vim:term "base unit" ;
    rdfs:subClassOf vim:MeasurementUnit .

vim:DerivedUnit a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "measurement unit for a derived quantity" ;
    vim:it 14 ;
    vim:term "derived unit" ;
    rdfs:subClassOf vim:MeasurementUnit .

vim:GeneralProperty a rdfs:Class ;
    vim:ch 0 ;
    vim:definition "(primitive)" ;
    rdfs:subClassOf vim:VIMEntity .

vim:GeneralQuantity a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "property for which comparability by ratio, or by difference, or by order applies" ;
    vim:it 1 ;
    vim:term "quantity <general>" ;
    rdfs:subClassOf vim:GeneralProperty .

vim:IndividualProperty a rdfs:Class ;
    vim:ch 0 ;
    vim:definition "(primitive)" ;
    rdfs:subClassOf vim:VIMEntity .

vim:IndividualQuantity a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "property for which comparability by ratio, or by difference, or by order applies" ;
    vim:it 1 ;
    vim:term "quantity <individual>" ;
    rdfs:subClassOf vim:IndividualProperty .

vim:MeasurementUnit a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "reference quantity with which any other quantity of the same kind can be compared by ratio, resulting in a number" ;
    vim:it 12 ;
    vim:term "measurement unit" ;
    rdfs:subClassOf vim:ReferenceQuantity .

vim:QuantityDimension a rdfs:Class ;
    vim:admitted_terms "dimension",
        "dimension of a quantity" ;
    vim:ch 1 ;
    vim:definition "relation of a quantity to the base quantities of a system of quantities as a product of the base quantities each raised to a power, omitting any proportionality factor" ;
    vim:it 7 ;
    vim:term "quantity dimension" ;
    rdfs:subClassOf vim:VIMEntity .

vim:ReferenceQuantity a rdfs:Class ;
    vim:ch 1 ;
    vim:definition "quantity identified and adopted by convention for empirically comparing it with other quantities of the same kind" ;
    vim:it 11 ;
    vim:term "reference quantity" ;
    rdfs:subClassOf vim:IndividualQuantity .

vim:VIMEntity a rdfs:Class ;
    vim:ch 0 ;
    vim:definition "(primitive)" ;
    vim:it 0 ;
    vim:term "VIM Entity" ;
    rdfs:subClassOf rdfs:Resource .

vim:admitted_terms a rdf:Property ;
    rdfs:label "The list of zero or more admitted terms of the entry" ;
    rdfs:domain vim:VIMEntity ;
    rdfs:range rdf:Seq .

vim:ch a rdf:Property ;
    rdfs:label "The id of the chapter in which the entry is included" ;
    rdfs:domain vim:VIMEntity ;
    rdfs:range rdfs:Literal .

vim:definition a rdf:Property ;
    rdfs:label "The definition of the entry" ;
    rdfs:domain vim:VIMEntity ;
    rdfs:range rdfs:Literal .

vim:duration a vim:GeneralQuantity ;
    vim:kind_name "duration" ;
    vim:kind_symbol "t" .

vim:it a rdf:Property ;
    rdfs:label "The id of the entry in the chapter" ;
    rdfs:domain vim:VIMEntity ;
    rdfs:range rdfs:Literal .

vim:kilogram a vim:MeasurementUnit ;
    vim:unit_kind "mass" ;
    vim:unit_name "kilogram" ;
    vim:unit_symbol "m" .

vim:length a vim:GeneralQuantity ;
    vim:kind_name "length" ;
    vim:kind_symbol "l" .

vim:mass a vim:GeneralQuantity ;
    vim:kind_name "mass" ;
    vim:kind_symbol "m" .

vim:metre a vim:MeasurementUnit ;
    vim:unit_kind "length" ;
    vim:unit_name "metre" ;
    vim:unit_symbol "m" .

vim:second a vim:MeasurementUnit ;
    vim:unit_kind "duration" ;
    vim:unit_name "second" ;
    vim:unit_symbol "s" .

vim:term a rdf:Property ;
    rdfs:label "The preferred term of the entry" ;
    rdfs:domain vim:VIMEntity ;
    rdfs:range rdfs:Literal .

